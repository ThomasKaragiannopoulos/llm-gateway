{
  "id": null,
  "uid": "llm-gateway",
  "title": "LLM Gateway",
  "tags": [
    "llm",
    "gateway"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 3,
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Gateway Up",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "max(up{job=\"gateway\"})",
          "legendFormat": "up"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Requests / sec",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "legendFormat": "rps"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Error Rate % (5m)",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "100 * sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m]))",
          "legendFormat": "5xx%"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "unit": "percent"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "P95 Latency (s)",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Requests by Path",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (path) (rate(http_requests_total[1m]))",
          "legendFormat": "{{path}}"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Latency p50/p95 (s)",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Errors & Rejections",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        },
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"4..\"}[5m]))",
          "legendFormat": "4xx"
        },
        {
          "expr": "sum(rate(rate_limited_total[5m]))",
          "legendFormat": "rate_limited"
        },
        {
          "expr": "sum(rate(quota_denied_total[5m]))",
          "legendFormat": "quota_denied"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Cache Hits / Misses",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(cache_hits_total[5m]))",
          "legendFormat": "hits"
        },
        {
          "expr": "sum(rate(cache_misses_total[5m]))",
          "legendFormat": "misses"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Tokens / sec",
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(tokens_total[1m]))",
          "legendFormat": "tokens/s"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Cost USD / sec",
      "gridPos": {
        "x": 12,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(cost_total[5m]))",
          "legendFormat": "usd/s"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Fallbacks by Reason",
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (reason, to_provider) (rate(fallback_total[5m]))",
          "legendFormat": "{{reason}} -> {{to_provider}}"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "RAG Retrieval Rate",
      "gridPos": {
        "x": 0,
        "y": 36,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(rag_retrieval_total[5m]))",
          "legendFormat": "{{status}}"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "RAG Retrieval Latency (p50/p95)",
      "gridPos": {
        "x": 12,
        "y": 36,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(rag_retrieval_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "RAG Context Tokens/Chunks",
      "gridPos": {
        "x": 0,
        "y": 44,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum(rate(rag_context_tokens_total[5m]))",
          "legendFormat": "tokens/s"
        },
        {
          "expr": "sum(rate(rag_context_chunks_total[5m]))",
          "legendFormat": "chunks/s"
        }
      ],
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    }
  ]
}